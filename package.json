{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A simple react boilerplate that use parcel and lint to standardize code.",
  "main": "index.js",
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ dist/",
    "dev": "parcel public/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "tslint --project",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "react-boilerplate"
  },
  "keywords": [
    "react",
    "typescript",
    "ts",
    "boilerplate",
    "parcel",
    "prettier",
    "lint"
  ],
  "author": "m-ossolinski",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.4",
    "sass": "^1.26.3",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style.css": "^1.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ]
}
